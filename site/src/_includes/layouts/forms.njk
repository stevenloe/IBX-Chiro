
{% extends "layouts/base.njk" %}
{% block meta %}
  <meta name="description" content="New patient forms for clients of Inner Banks Chiropractic. Office of Dr. Shari Richard located in Oriental, NC. Please fill these out prior to your first visit.">
{% endblock %}
{% block customstyles %}
<style>
  body {
    background: url({{ bodyImg }});
    background-position: {{ bodyImgPosition }};
    background-repeat: no-repeat;
    background-attachment: fixed; 
  }

</style>
{% endblock %}

{% block hero %}
  <div class="hero">
    {% include "components/owner.njk" %}

    <header class="cta">
      <h1>{{ heroText }}</h1>
    </header>
      
  </div>
{% endblock %}
